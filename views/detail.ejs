<%- include('app/head') %>

<article data-section="detail" data-id="<%= object.id %>">
	<a href="<%= back %>">Go back to the collection</a>
	<img src="<%= object.webImage.url %>" alt="<%= object.longTitle %>" data-guid="<%= object.webImage.guid %>">
	<h1><%= object.longTitle %></h1>
	<p><%= object.label.description %></p>
</article>

<%- include('app/foot') %>
